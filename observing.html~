	$('a.button.icon.fa-arrow-circle-o-right').click(
		function(){
			$('#hidden').slideToggle(function(){
				var state=$('a.button.icon.fa-arrow-circle-o-right').text();
				if (state === "SHOW PAST SCHEDULES"){
					$('a.button.icon.fa-arrow-circle-o-right').text("HIDE PAST SCHEDULES");
				} else {
					$('a.button.icon.fa-arrow-circle-o-right').text("SHOW PAST SCHEDULES");
				}
			
			});
		});
	</script>
	</body>
</html>
